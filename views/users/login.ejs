<%layout("layout/boilerplate.ejs")%>
<h1>Login on Wanderlust</h1>

<div class="login">
    <form method="post" action="/login" novalidate  class="needs-validation">
        <div class="form-floating mb-3">
            <input type="username" name="username" class="form-control" id="username" placeholder="username" required>
            <label for="username">Username</label>
            <div class="invalid-feedback">Please enter  review</div>

          </div>
        
          <div class="form-floating">
            <input type="password" name="password" class="form-control" id="floatingPassword" placeholder="Password" required>
            <label for="floatingPassword">Password</label>
            <div class="invalid-feedback">Please enter  review</div>

          </div>
        <!-- <div class="form-floating mt-3"> -->
            <button class="btn btn-outline-success mt-3">SignIn</button>
        <!-- </div> -->
          
    </form>
</div>